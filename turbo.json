{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [],
    "globalPassThroughEnv": [],
    "globalEnv": [],
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "dev": {
            "dependsOn": [
                "^build"
            ],
            "cache": false,
            "persistent": true
        },
        "lint": {},
        "clean": {
            "cache": false
        }
    }
}