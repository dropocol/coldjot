{
    "name": "@mailjot/database",
    "version": "0.0.0",
    "private": true,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf .turbo node_modules dist",
        "db:generate": "prisma generate",
        "db:push": "node -r ts-node/register ./prisma.config.ts && prisma db push --skip-generate",
        "db:migrate": "node -r ts-node/register ./prisma.config.ts && prisma migrate dev",
        "db:deploy": "node -r ts-node/register ./prisma.config.ts && prisma migrate deploy",
        "db:reset": "node -r ts-node/register ./prisma.config.ts && prisma migrate reset --force",
        "db:seed": "node -r ts-node/register ./prisma.config.ts && prisma db seed",
        "dev": "tsup --watch",
        "lint": "eslint .",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@prisma/client": "^5.7.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.4",
        "dotenv": "^16.4.7",
        "prisma": "^5.7.0",
        "ts-node": "^10.9.1",
        "rimraf": "^5.0.5",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
}