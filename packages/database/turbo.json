            "cache": false,
            "persistent": false,
            "dependsOn": [
                "^build",
                "db:generate"
            ]
        },
        "build:prod": {
            "dependsOn": [
                "^build:prod",
                "db:generate"
            ]
        },
        "db:generate": {
            "cache": false
        },
        "db:migrate": {
            "cache": false
        },
        "db:migrate:create": {
            "cache": false
        },
        "db:deploy": {
            "cache": false
        },
        "db:push": {
            "cache": false
        },
        "db:seed": {
            "cache": false
        },
        "db:reset": {
            "cache": false
        },
        "db:studio": {
            "cache": false
        },
        "db:deploy:prod": {
            "cache": false
        },
        "db:seed:prod": {
            "cache": false
        }
    }
